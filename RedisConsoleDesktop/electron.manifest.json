{
  "executable": "RedisConsoleDesktop",
  "splashscreen": {
    "imageFile": "/Assets/about.png"
  },
  "name": "RedisConsoleDesktop",
  "author": "Christos Christodoulidis",
  "singleInstance": true,
  "build": {
    "appId": "com.RedisConsole.app",
    "productName": "RedisConsoleDesktop",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "0.2.0",
    "compression": "maximum",
    "mac": {
      "icon": "./icon.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "imgs/apple-touch-icon.png"
    },
    "directories": {
      "output": "../../../bin/Desktop",
      "buildResources": "Assets"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}